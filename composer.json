{
    "name": "xandco/domainparser",
    "description": "Domain Parser, parses and validates a domain name.",
    "license": "MIT",
    "authors": [
        {
            "name": "Miguel Batres Santellano",
            "email": "miguel@xand.co",
            "homepage": "https://xand.co"
        }
    ],
    "homepage": "https://github.com/xandco/domainparser",
    "keywords": ["Laravel", "DomainParser"],
    "require": {
        "ext-json": "*",
        "illuminate/support": "~5|~6|~7",
        "true/punycode": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4"
    },
    "autoload": {
        "psr-4": {
            "xandco\\DomainParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "xandco\\DomainParser\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "xandco\\DomainParser\\DomainParserServiceProvider"
            ],
            "aliases": {
                "DomainParser": "xandco\\DomainParser\\Facades\\DomainParser"
            }
        }
    }
}
